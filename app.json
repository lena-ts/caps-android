      "permissions": ["READ_CONTACTS", "RECORD_AUDIO", "READ_PHONE_STATE", "CAMERA"],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3485393765261217~5865486809"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
